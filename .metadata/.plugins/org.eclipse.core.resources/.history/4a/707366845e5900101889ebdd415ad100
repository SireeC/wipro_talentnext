package java_fundamentals;

public class FCS13 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int count=0;
		for(int i=1;i<=99;i++) {
			boolean isPrime=true;
			for(int j=2;j<i/2;j++) {
				if(i%j==0) {
					isPrime=false;
				}
			}
		
			if(isPrime && i>1) {
				System.out.println(i+ " ");
				count++;
			}
		}
		System.out.println("The total number of prime numbers are: " +count);
	}

}
