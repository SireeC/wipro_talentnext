package java_fundamentals;

public class Question4A {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=-2;
		if(a==0) {
			System.out.println("Zero");
		}
		else if(a>0) {
			System.out.println("Positive");
		}
		else {
			System.out.println("Negative");
		}

	}

}
